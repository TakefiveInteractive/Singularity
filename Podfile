    xcodeproj 'GoogleVoiceRecognition/GoogleVoiceRecognition.xcodeproj'
    pod 'Alamofire'
    pod 'SwiftyJSON', :git => 'https://github.com/SwiftyJSON/SwiftyJSON.git'
    pod 'PromiseKit'
end
