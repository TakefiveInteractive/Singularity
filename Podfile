workspace 'Singularity.xcworkspace'
source 'https://github.com/CocoaPods/Specs.git'
platform :osx, '10.11'
use_frameworks!

xcodeproj 'Singularity-Desktop/Singularity-Desktop.xcodeproj'
pod 'EZAudio'
